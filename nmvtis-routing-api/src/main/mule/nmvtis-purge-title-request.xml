http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
	<sub-flow name="purge-title-request-subflow" doc:id="d7a159d6-be38-4883-a8a4-b994dd45c2bf" >
		<set-variable value="#[attributes.headers.Authorization]" doc:name="var-authorization" doc:id="a71472cd-29b6-4c26-9aa7-9a6d333c1379" variableName="Authorization" />
		<http:request method="POST" doc:name="call-aamva" doc:id="cb7f6cc1-b7e4-406a-802e-b56f1a295560" config-ref="HTTP_AAMVA_Request_Configuration" path="${aamva-request.nmvtis.purge-title-path}" >
			<http:headers ><![CDATA[#[output application/java
---
{
	"Authorization" : vars.Authorization
}]]]></http:headers>
		</http:request>
		<ee:transform doc:name="response" doc:id="f3be5b03-9e18-4a40-b13d-0d6b97e385f7" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
payload]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</sub-flow>
</mule>
