#%RAML 1.0
title:  nmvits routing api
baseUri: /api
description: Route request to AAMVA and receive a response from AAMVA
version: v1
protocols: [HTTPS]
mediaType: 
- application/json


/v1/NewVehicleInquiry:
  post:
    displayName: Send NewVehicleInquiry Request to AAMVA-NMVTIS

/v1/NewVehicleInquiryResponse:
  post:
    displayName: Recieve NewVehicleInquiry Response from AAMVA-NMVTIS

/v1/UsedVehicleInquiry:
  post:
