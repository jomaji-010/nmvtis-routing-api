    displayName: Recieve UsedVehicleInquiry Response from AAMVA-NMVTIS

/v1/StateVehicleDataRequest:
  post:
    displayName: Recieve unsolicited State Vehicle data request from AAMVA.

/v1/StateVehicleDataResponse:
  post:
    displayName: Send unsolicited State Vehicle Response to AAMVA-NMVTIS.

