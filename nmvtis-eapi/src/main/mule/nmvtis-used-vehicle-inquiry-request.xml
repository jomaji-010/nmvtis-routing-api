		<http:request method="${nmvtis-sf-solicited-sapi.method}" doc:name="call-solicited-SAPI" doc:id="298cd67a-d426-495f-b8eb-9f28503d3951" config-ref="nmvtis-sf-solicited-sapi" path="${nmvtis-sf-solicited-sapi.usedVehicleInquiryPath}">
			<http:response-validator >
